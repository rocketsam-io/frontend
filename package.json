{
  "name": "rocket-man",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "vite preview",
    "dev": "vite --host",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "generate-types": "typechain --target=ethers-v6 './src/abi/**/*.json' --out-dir ./src/types/ethers-contracts",
    "deploy_dev": "git add . && git commit -m 'deploy to develop' && git push origin main && caprover deploy -n rocket -a develop --default && npm run logs_dev",
    "logs_dev": "ssh root@captain.panel.rocketsam.io 'docker service logs srv-captain--develop --follow --no-task-ids'",
    "deploy_production": "caprover deploy -n rocket -a frontend --default && npm run logs_production",
    "logs_production": "ssh root@captain.panel.rocketsam.io 'docker service logs srv-captain--production --follow --no-task-ids'"
  },
  "dependencies": {
    "@vueuse/components": "^10.6.1",
    "@vueuse/core": "^10.6.1",
    "@web3-onboard/core": "^2.21.2",
    "@web3-onboard/injected-wallets": "^2.10.9",
    "@web3-onboard/vue": "^2.7.12",
    "@web3-onboard/walletconnect": "^2.5.0",
    "axios": "^1.6.0",
    "bulma": "^0.9.4",
    "ethers": "^6.8.1",
    "ethers-multicall": "^0.2.3",
    "inter-ui": "^3.19.3",
    "pinia": "^2.1.7",
    "vue": "^3.3.4",
    "vue-router": "^4.2.5",
    "vue-toastification": "2.0.0-rc.5",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node18": "^18.2.2",
    "@typechain/ethers-v6": "^0.5.1",
    "@types/node": "^18.18.5",
    "@vitejs/plugin-vue": "^4.4.0",
    "@volar/vue-language-plugin-pug": "^1.6.5",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "npm-run-all2": "^6.1.1",
    "prettier": "^3.0.3",
    "pug": "^3.0.2",
    "sass": "^1.69.5",
    "typechain": "^8.3.2",
    "typescript": "~5.2.0",
    "vite": "^4.4.11",
    "vue-pug-plugin": "^2.0.3",
    "vue-tsc": "^1.8.19"
  }
}
